{
  "name": "@boxyhq/saml20",
  "version": "1.5.1",
  "description": "SAML 2.0 token parser for Node.js",
  "keywords": [
    "SAML 2.0",
    "Token Parser"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/boxyhq/saml20.git"
  },
  "license": "MIT",
  "author": "Leandro Boffi (me@leandrob.com)",
  "contributors": [
    {
      "name": "Phillip Son",
      "email": "philson3@gmail.com"
    },
    {
      "name": "Deepak Prabhakara",
      "email": "deepak@boxyhq.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "sort": "npx sort-package-json",
    "test": "nyc mocha --check-leaks  -r tsconfig-paths/register -r ts-node/register test/lib/**/*.spec.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "npm run build",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "release": "git checkout release && git merge origin/main && release-it && git checkout main && git merge origin/release && git push origin main"
  },
  "dependencies": {
    "@xmldom/xmldom": "0.8.10",
    "xml-crypto": "6.0.0",
    "xml-encryption": "3.0.2",
    "xml2js": "0.6.2",
    "xmlbuilder": "15.1.1"
  },
  "devDependencies": {
    "@types/mocha": "10.0.9",
    "@types/node": "22.7.5",
    "@types/xml2js": "0.4.14",
    "@typescript-eslint/eslint-plugin": "8.9.0",
    "@typescript-eslint/parser": "8.9.0",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "mocha": "10.7.3",
    "nyc": "17.1.0",
    "prettier": "3.3.3",
    "release-it": "17.9.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3"
  },
  "readmeFilename": "README.md"
}
