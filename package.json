{
  "name": "@boxyhq/saml20",
  "version": "1.1.0",
  "description": "SAML 2.0 token parser for Node.js",
  "keywords": [
    "SAML 2.0",
    "Token Parser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boxyhq/saml20.git"
  },
  "license": "MIT",
  "author": "Leandro Boffi (me@leandrob.com)",
  "contributors": [
    {
      "name": "Phillip Son",
      "email": "philson3@gmail.com"
    },
    {
      "name": "Deepak Prabhakara",
      "email": "deepak@boxyhq.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "sort": "npx sort-package-json",
    "test": "nyc mocha --check-leaks  -r tsconfig-paths/register -r ts-node/register test/lib/**/*.spec.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  },
  "dependencies": {
    "@xmldom/xmldom": "0.8.6",
    "lodash": "4.17.21",
    "rambda": "7.4.0",
    "xml-crypto": "3.0.1",
    "xml-encryption": "3.0.2",
    "xml2js": "0.4.23",
    "xmlbuilder": "15.1.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.17",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.1",
    "chai": "4.3.7",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "mocha": "10.1.0",
    "nyc": "15.1.0",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.4"
  },
  "readmeFilename": "README.md"
}