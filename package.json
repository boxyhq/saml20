  "dependencies": {
    "@xmldom/xmldom": "0.8.10",
    "lodash": "4.17.21",
    "rambda": "8.3.0",
    "xml-crypto": "4.1.0",
    "xml-encryption": "3.0.2",
    "xml2js": "0.6.2",
    "xmlbuilder": "15.1.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.5",
    "@types/mocha": "10.0.1",
    "@types/node": "20.4.6",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.3.0",
    "chai": "4.3.7",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "prettier": "3.0.0",
    "release-it": "16.1.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "readmeFilename": "README.md"
