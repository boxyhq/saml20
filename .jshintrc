{
  "browser" : true,
  "node" : true,

  "bitwise" : true,
  "camelcase" : false,
  "curly" : true,
  "eqeqeq" : true,
  "forin" : true,
  "freeze" : true,
  "immed" : true,
  "indent" : 2,
  "latedef" : "nofunc",
  "maxcomplexity" : 18,
  "newcap" : true,
  "noarg" : true,
  "nonew" : true,
  "plusplus" : false,
  "quotmark" : "ignore",
  "strict" : true,
  "trailing" : true,
  "undef" : true,
  "unused" : "vars",

  "globalstrict" : true,
  "laxbreak" : true,

  "globals" : {
    "after" : true,
    "afterEach" : true,
    "angular" : true,
    "before" : true,
    "beforeEach" : true,
    "define" : true,
    "describe" : true,
    "expect" : true,
    "it" : true,
    "URI" : true
  }
}
